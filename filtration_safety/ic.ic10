alias pressure r1
define MAX_PRESSURE 40000
define ACTIVE 1
define IDLE 0

loop:
l pressure db PressureInput
blt pressure 1 disable
l pressure db PressureOutput
bgt pressure MAX_PRESSURE disable
l pressure db PressureOutput2
bgt pressure MAX_PRESSURE disable
j enable

enable:
s db Mode ACTIVE
j end

disable:
s db Mode IDLE
j end

end:
sleep 10
j loop
