define MAX_COUNT 300
define H_CENTRIFUGE HASH("StructureCentrifuge")

alias reagents r1

loop:
lb reagents H_CENTRIFUGE Reagents Maximum
s db Setting reagents
bgt reagents MAX_COUNT open
sleep 300
j loop

open:
sb H_CENTRIFUGE Open 1
sleep 60
sb H_CENTRIFUGE Open 0
sb H_CENTRIFUGE ClearMemory 1
j loop